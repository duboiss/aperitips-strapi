{
  "kind": "collectionType",
  "collectionName": "blog_posts",
  "info": {
    "name": "BlogPost"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "publishedAt": {
      "type": "date"
    },
    "description": {
      "type": "text"
    },
    "content": {
      "type": "richtext"
    }
  }
}
